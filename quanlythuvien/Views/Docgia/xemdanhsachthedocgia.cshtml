@model List<Models.Framework.THEDOCGIA>

@{
    ViewBag.Title = "xemdanhsachthedocgia";
}

<h2>xemdanhsachthedocgia</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Ngay Sinh</th>
            <th scope="col">DiaChi</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MaTheDocGia</td>
                <td>@item.HoTenDocGia</td>
                <td>@item.NgaySinh</td>
                <td>@item.DiaChi</td>

            </tr>
        }


    </tbody>
</table>
